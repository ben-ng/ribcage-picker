<div class="rp-wrapper">
  <div class="rp-header">
    <div class="rp-cancel">Cancel</div>
    <div class="rp-done">Done</div>
  </div>
  <div class="rp-slots-wrapper">
    <div class="rp-slots">
    {{#each slots}}
      <div class="rp-{{style}}">
        <ul class="picker-slot-{{@key}}">
          {{#each values}}
            <li>{{@key}}</li>
          {{/each}}
        </ul>
      </div>
    {{/each}}
    </div>
  </div>
  <div class="rp-frame"></div>
  </div>
</div>
